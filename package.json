{
  "name": "node-commander",
  "version": "1.0.0",
  "description": "",
  "main": "src/nw/nw-loader.html",
  "scripts": {
    "web-dev": "concurrently \"node src/server/server.js\" \"gulp compile-stylus:watch\"",
    "nw": "/Applications/nwjs.app/Contents/MacOS/nwjs .",
    "webpack": "webpack -w",
    "dist": "build --tasks mac-x64,win-x64 --mirror https://dl.nwjs.io/ .",
    "start": "run --x64 --mirror https://dl.nwjs.io/ ."
  },
  "window": {
    "title": "Node Commander",
    "toolbar": true,
    "frame": true,
    "width": 1000,
    "height": 800,
    "min_width": 400,
    "min_height": 400
  },
  "author": "",
  "license": "ISC",
  "build": {
    "nwVersion": "0.22.1",
    "excludes": [
      "./src/client/**/*"
    ],
    "output": "./nw-dist/"
  },
  "dependencies": {
    "body-parser": "^1.17.1",
    "classnames": "^2.2.5",
    "express": "^4.15.3",
    "ncp": "^2.0.0",
    "open": "0.0.5",
    "stylus": "^0.54.5"
  },
  "devDependencies": {
    "babel": "6.23.0",
    "babel-cli": "6.24.1",
    "babel-core": "6.23.1",
    "babel-loader": "6.3.2",
    "babel-plugin-module-alias": "1.6.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-flow": "6.23.0",
    "babel-preset-latest": "6.22.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-0": "6.22.0",
    "babel-runtime": "6.23.0",
    "chokidar": "1.6.1",
    "gulp": "3.9.1",
    "gulp-inject": "4.2.0",
    "gulp-sort": "2.0.0",
    "gulp-stylus": "2.6.0",
    "npm-install-webpack-plugin": "4.0.4",
    "webpack": "2.2.1",
    "nwjs-builder-phoenix": "^1.10.2"
  }
}
